{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "../wwwroot/",
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "skipLibCheck": true,
    "paths": {
      "*": [
        "*",
        "app/*"
      ]
    },
    "noUnusedLocals": true,
    "suppressImplicitAnyIndexErrors": true,
    "typeRoots": [
      "../node_modules/@types"
    ]
  }
}